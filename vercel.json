{
    “version”: 2,
    “builds”: [
        {
    “src”:”poll/wsgi.py”,
    “use”: “@versal/python”,
    “config” : {“maxLambdaSize”: “15mb”, “runtime”: “python3.9”
            }
        },
        {
    “src”: “build_files.sh”,
    “use”: “@vercel/static-build”,
    “config”: {“distDir”: “staticfiles_build”
            }
        }
    ],
    “routes”: [
        {
    “src”: “/static/(.*)”,
    “dest”: “poll/wsgi.py”
        }
    ]
}